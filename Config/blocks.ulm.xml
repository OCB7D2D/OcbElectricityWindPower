<configs>
	<append xpath="/blocks/block[@name='ocbElectricityRusticWindmill']">
		<property name="UnlockedBy" value="ocbElectricityRusticWindmillBlueprint"/>

		<property name="CarryWeight" value="20"/>
		<property name="Stacknumber" value="5"/>

		<property name="OutputPerStack" value="30"/>
		<property name="MaxPower" value="180"/>

		<property class="RepairItems">
			<property name="resourceMechanicalParts" value="3"/>
			<property name="resourceElectricParts" value="3"/>
			<property name="resourceScrapIron" value="10"/>
			<property name="ulmResourcePlank" value="10"/>
		</property>

		<drop event="Harvest" name="air" count="0" tool_category="Disassemble"/>

		<drop event="Harvest" name="ulmResourceScrapSteel" count="10,20" tag="allHarvest"/>
		<drop event="Harvest" name="ulmResourceScrapSteel" count="10,20" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceElectricParts" count="1,2" tag="salvage0,salvageHarvest"/>
		<drop event="Harvest" name="resourceScrapIron" count="4,8" tag="salvage0,salvageHarvest"/>
		<drop event="Harvest" name="resourceMechanicalParts" count="1,2" tag="salvage1,salvageHarvest"/>

		<drop event="Harvest" name="ulmResourceElectronicFuse" prop="0.15" tag="salvageHarvest"/>
		<drop event="Harvest" name="ulmResourceElectronicSwitch" prop="0.15" tag="salvageHarvest"/>
		<drop event="Harvest" name="ulmResourceElectronicCircuitboard" prop="0.05" tag="salvageHarvest"/>

		<drop event="Destroy" count="0"/>

		<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
	</append>

	<append xpath="/blocks/block[@name='ocbElectricityModernWindmill']">
