		<drop event="Harvest" name="air" count="0" tool_category="Disassemble"/>

		<drop event="Harvest" name="ulmResourceScrapSteel" count="30,60" tag="allHarvest"/>
		<drop event="Harvest" name="ulmResourceScrapSteel" count="30,60" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceElectricParts" count="2,4" tag="salvage0,salvageHarvest"/>
		<drop event="Harvest" name="resourceMechanicalParts" count="3,6" tag="salvage1,salvageHarvest"/>
		<drop event="Harvest" name="resourceScrapPolymers" count="2,8" tag="salvage2,salvageHarvest"/>

